{
    "name": "henzeb/closure",
    "description": "Invoke Fully Qualified Class Name strings of invokable classes!",
    "keywords": [
        "henzeb",
        "closure",
        "callable",
        "invokable",
        "FQCN"
    ],
    "homepage": "https://github.com/henzeb/closure",
    "license": "AGPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "Henze Berkheij",
            "email": "henzeberkheij@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "infection/infection": "^0.26.19",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.6.6|^10"
    },
    "autoload": {
        "files": [
            "src/Closure.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Henzeb\\Closure\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage-txt": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage",
        "test-dox": "vendor/bin/phpunit --testdox",
        "infection": "vendor/bin/infection --git-diff-filter=AM"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
    }
}
